<div class="span12">
<table id="address-book" class="table">
  <thead>
    <tr>
      <% Settings.sections.each do |section| %>
        <% section.fields.each do |field| %>
          <% unless field.addressbook_ignore %>
            <% values = @user_entry[field.name.to_sym].length < 1 ? [""] : @user_entry[field.name.to_sym][0..0] %> 
            <% values.each do |value| %>
              <th><%= field.label %></th>
            <% end %>
          <% end %>
        <% end %>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <% Settings.sections.each do |section| %>
          <% section.fields.each do |field| %>
            <% unless field.addressbook_ignore %>
              <% values = user[field.name.to_sym].length < 1 ? [""] : user[field.name.to_sym][0..0] %> 
              <% values.each do |value| %>
                <td>
                  <%= value.force_encoding('UTF-8') %>
                </td>
              <% end %>
            <% end %>
          <% end %>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
